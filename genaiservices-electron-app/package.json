{
  "name": "genaiservices-electron-app",
  "version": "0.1.0",
  "description": "This app allows you to display ChatGPT, Claude, and Gemini on different tabs or side by side for comparison",
  "main": "dist/app.bundle.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf dist",
    "build-main": "webpack --config webpack.main.config.js",
    "build-renderer": "webpack --config webpack.renderer.config.js",
    "prune": "npm prune --production",
    "build": "npm run clean && npm run build-main && npm run build-renderer && npm prune && electron-builder -mw",
    "build-mac": "npm run clean && npm run build-main && npm run build-renderer && npm prune && electron-builder -m",
    "build-win": "npm run clean && npm run build-main && npm run build-renderer && npm prune && electron-builder -w",
    "build-unpacked": "npm run clean && npm run build-main && npm run build-renderer && electron-builder --dir",
    "build-dev": "npm run clean && npm run build-main && npm run build-renderer && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "electron": "^34.2.0",
    "electron-build-service": "^1.0.7",
    "electron-builder": "^24.3.0",
    "eslint": "^9.20.1",
    "mini-css-extract-plugin": "^2.9.2",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-jsx": "^5.3.2",
    "ajv": "^6.12.6",
    "ansi-styles": "^4.3.0",
    "argparse": "^2.0.1",
    "balanced-match": "^1.0.2",
    "brace-expansion": "^1.1.11",
    "callsites": "^3.1.0",
    "chalk": "^4.1.2",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "concat-map": "^0.0.1",
    "cross-spawn": "^7.0.6",
    "debug": "^4.4.0",
    "deep-is": "^0.1.4",
    "escape-string-regexp": "^4.0.0",
    "eslint-scope": "^8.2.0",
    "eslint-visitor-keys": "^4.2.0",
    "espree": "^10.3.0",
    "esquery": "^1.6.0",
    "esrecurse": "^4.3.0",
    "estraverse": "^5.3.0",
    "esutils": "^2.0.3",
    "fast-deep-equal": "^3.1.3",
    "fast-json-stable-stringify": "^2.1.0",
    "fast-levenshtein": "^2.0.6",
    "file-entry-cache": "^8.0.0",
    "find-up": "^5.0.0",
    "flat-cache": "^4.0.1",
    "flatted": "^3.3.2",
    "glob-parent": "^6.0.2",
    "globals": "^14.0.0",
    "has-flag": "^4.0.0",
    "ignore": "^5.3.2",
    "import-fresh": "^3.3.1",
    "imurmurhash": "^0.1.4",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "isexe": "^2.0.0",
    "js-yaml": "^4.1.0",
    "json-buffer": "^3.0.1",
    "json-schema-traverse": "^0.4.1",
    "json-stable-stringify-without-jsonify": "^1.0.1",
    "keyv": "^4.5.4",
    "levn": "^0.4.1",
    "locate-path": "^6.0.0",
    "lodash.merge": "^4.6.2",
    "minimatch": "^3.1.2",
    "ms": "^2.1.3",
    "natural-compare": "^1.4.0",
    "optionator": "^0.9.4",
    "p-limit": "^3.1.0",
    "p-locate": "^5.0.0",
    "parent-module": "^1.0.1",
    "path-exists": "^4.0.0",
    "path-key": "^3.1.1",
    "prelude-ls": "^1.2.1",
    "punycode": "^2.3.1",
    "resolve-from": "^4.0.0",
    "shebang-command": "^2.0.0",
    "shebang-regex": "^3.0.0",
    "strip-json-comments": "^3.1.1",
    "supports-color": "^7.2.0",
    "type-check": "^0.4.0",
    "uri-js": "^4.4.1",
    "which": "^2.0.2",
    "word-wrap": "^1.2.5",
    "yocto-queue": "^0.1.0"
  },
  "build": {
    "appId": "com.example.genaiservices.electron.app",
    "productName": "GenAI Services",
    "directories": {
      "output": "dist/prod"
    },
    "win": {
      "target": "nsis",
      "icon": "resources/pyleiton-icon.ico",
      "compression": "maximum",
      "executableName": "genai-services",
      "artifactName": "genai-services-${version}-${os}-${arch}.${ext}"
    },
    "mac": {
      "target": "dmg",
      "icon": "resources/pyleiton-icon.icns",
      "category": "public.app-category.utilities",
      "artifactName": "genai-services-${version}-${os}-${arch}.${ext}"
    },
    "dmg": {
      "icon": "resources/icon.icns",
      "iconSize": 80,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "resources/**/*",
      "dist/**/*",
      "index.html",
      "src/**/*",
      "styles.css",
      "styles.min.css",
      "package.json"
    ],
    "extraResources": [
    {
      "from": "resources/",
      "to": ".",
      "filter": [
        "**/*"
      ]
    }
  ],
    "asar": true,
    "npmRebuild": false
  }
}
